class Solution:
    def canPlaceFlowers(self, flowerbed: List[int], n: int) -> bool:
        s = len(flowerbed)

        for i in range(0, s):
            if flowerbed[i] == 0:
                left_empty =  (i == 0 or flowerbed[i - 1] == 0)
                right_empty =  (i == s - 1 or flowerbed[i + 1] == 0)

                if left_empty and right_empty:
                    flowerbed[i] = 1
                    n -= 1

                    if n == 0:
                        return True
                
        return n <= 0
// 注意不能直接返回false，不然，n=0时的用例过不了。



        
